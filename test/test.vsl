FUNC main()
{
    // This is a comment
	VAR i  // declaration must be ahead of statement
	VAR a, b, c  // You can declare more than one variable in a declaration
	i := 0
	a := 8
	b := 2
	c := a / b  // only support integer division
    WHILE 11 - i
	DO
	{
		PRINT "f( ", i, " ) = ", f(i),  "\n"
		i := i + 1
	}
	DONE
}
FUNC f(n)
{
	IF n
	THEN
		RETURN n * f(n - 1)
	ELSE
		RETURN 1
	FI
}